<%# form_for @post do |f| %>
  <%# f.text_field :title %>
  <%# f.text_area :content %>
  <%# f.submit %>
<%# end %>
<div>
  <%= simple_form_for @post, html: {multipart: true} do |f| %>
    <%= f.error_notification %>
    <%= f.input :title %>
    <%= f.input :content %>
    <%= f.input :img %>
    <%= f.button :submit, class: "btn-primary" %>
  <% end %>
</div
